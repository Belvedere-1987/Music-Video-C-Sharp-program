@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<body onload="moveLZ()"> 
    <div class="text-center">
        <div id="LZ">LZ</div>
        <h1 class="display-4">Welcome</h1>
            <p>Check out Led Zeppelin's Over the Hills and Far Away:</a>.</p>
            <div class="video">
             <iframe width="656" height="492" src="https://www.youtube.com/embed/o-tT62bpYlU" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen>
             </iframe>
            </div> 
    </div>
</body> 

<script>
    function moveLZ() {
        var font = document.getElementById("LZ");
        var pos = 0; 
        var id = setInterval(frame, 10);
        function frame() {
            if (pos == 800) {
                clearInterval(id);
            } else {
                pos++; 
                font.style.top = pos + 'px'; 
                font.style.left = pos + 'px';
            }
        }
    }
</script>